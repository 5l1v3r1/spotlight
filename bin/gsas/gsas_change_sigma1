#! /bin/bash

HIST=${1}
PHASE=${2}
VAL=${3}

echo "k l o p h ${HIST} p ${PHASE}" > temp.txt
echo "c" >> temp.txt
echo "/" >> temp.txt
echo "/" >> temp.txt
echo ". ${VAL} ." >> temp.txt
echo "/" >> temp.txt
echo "/" >> temp.txt
echo "x" >> temp.txt
echo "x" >> temp.txt
echo "x" >> temp.txt
echo "x" >> temp.txt
expedt `cat GSAS_EXP` < temp.txt
